{
  "name": "react-debounced-props",
  "version": "0.1.0",
  "description": "higher order component for debouncing React component updates for props",
  "author": "saltycrane",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saltycrane/react-debounced-props.git"
  },
  "keywords": [
    "debounce",
    "react"
  ],
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "peerDependencies": {
    "react": ">=15.0"
  },
  "//": [
    "react, react-dom, and babel-polyfill are included in devDependencies for unit tests"
  ],
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "jest": "22.4.3",
    "react": "16.3.2",
    "react-dom": "16.3.2"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/node_modules/babel-polyfill/browser.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js"
  }
}
